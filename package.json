{
  "name": "terminal-emulator",
  "version": "1.0.0",
  "description": "A modern terminal emulator built with React Router v7, TypeScript, and TailwindCSS featuring Unix commands, I/O redirection, and Catppuccin theme",
  "keywords": [
    "terminal",
    "emulator",
    "react-router",
    "typescript",
    "catppuccin",
    "unix",
    "shell"
  ],
  "author": "ThibaultJRD",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "start:local": "react-router-serve ./build/server/nodejs_eyJydW50aW1lIjoibm9kZWpzIn0/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,html,json}' --config ./.prettierrc.json",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,html,json}' --config ./.prettierrc.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "audit": "yarn audit",
    "audit:fix": "yarn audit --fix || npm audit fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/react-router": "^1.2.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
